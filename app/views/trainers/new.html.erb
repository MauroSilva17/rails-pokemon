<div class="container">
  <div class="header">
    <%= link_to "< All Trainers>", trainers_path %>
    <h1>Sign up to train pokemon's</h1>
  </div>
    <%= simple_form_for @trainer, html: {mulitpart: true} do |f|%>
      <%= f.input :name %>
      <%= f.input :age %>
      <%= f.input :photo %>
      <%= f.button :submit, class: "btn btn-primary" %>
    <% end %>
</div>
